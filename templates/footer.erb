<%if options[:stub]%>
if __FILE__ == $0
  exit  if ARGV.size != 1
  filename = ARGV.shift
  rex = <%=target.last%>.new
  begin
    rex.load_file  filename
    while  token = rex.next_token
      p token
    end
  rescue
    $stderr.printf  "%s:%d:%s\n", rex.filename, rex.lineno, $!.message
  end
end<%end%>